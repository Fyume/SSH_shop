<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xmlns:aop="http://www.springframework.org/schema/aop" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd
        http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
        ">
<bean id="managerAction" class="zhku.jsj141.action.manager.ManagerAction" scope="prototype">
	<property name="managerService" ref="managerService"></property>
</bean>
<bean id="managerService" class="zhku.jsj141.service.Impl.ManagerServiceImpl">
	<property name="managerDao" ref="managerDao"></property>
</bean>
<bean id="managerDao" class="zhku.jsj141.dao.Impl.ManagerDaoImpl">
	<property name="hibernateTemplate" ref="hibernateTemplate"></property>
</bean>
</beans>